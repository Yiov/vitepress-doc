{
    "compilerOptions": {
      "outDir": "dist",
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "allowJs": true,
      "strict": true,
      "jsx": "preserve",
      "baseUrl": ".",
      "paths": {
        "@theme/*": [".vitepress/theme/components/*"]
      }
    },
    "include": ["env.d.ts", "src/**/*", "docs/.vitepress/**/*"]
  }